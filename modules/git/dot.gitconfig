[user]
  name = Masato Bito
  email = masato@bz2.jp

[core]
  whitespace=fix,-indent-with-non-tab,trailing-space,-cr-at-eol,space-before-tab
  pager = less -r

[color]
  ui = true

[push]
  default = simple

[url "git@github.com:"]
  pushInsteadOf = git://github.com/
  pushInsteadOf = https://github.com/

[url "git://github.com/"]
  insteadOf = https://github.com/

[ghq]
  root = ~/.ghq
  root = ~/.go/src
